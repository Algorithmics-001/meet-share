{
  "manifest_version": 3,
  "name": "Google Meet URL Extension",
  "version": "1.0",
  "description": "Get the URL of the current Google Meet",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  }
}
